{res}{txt}
{com}.         rename          ag3b_17 irrigated 
{res}{txt}
{com}.         rename          ag3b_45 fert_any
{res}{txt}
{com}.         rename          ag3b_47 kilo_fert
{res}{txt}
{com}.         rename          ag3b_58 pesticide_any
{res}{txt}
