{txt}
{com}. 
. * labor inputs
.         egen            hh_labor_days = rsum(ag3b_70_1 ag3b_70_2 ag3b_70_3 ag3b_70_4 ag3b_70_5 ag3b_70_6 ag3b_70_13 ag3b_70_14 ///
